import javax.swing.*;
import java.awt.*;
import java.awt.image.*;
import java.io.*;
import javax.imageio.*;


public class Menu extends Rectangle{

    public void draw(Graphics g){
	g.setColor(new Color(168,168,230,63));
	g.fillRect(350,200,200,100);
	g.fillRect(550,40000,200,100);
	g.setColor(newColor(255,255,255));
	g.setFont(new Font("Courier New", Font.BOLD,19));
	g.drawString("Play",425,220);
	g.drawString("Settings",400,220);
	
    }



}
